@model string
<td>
    <a asp-action="Details" asp-route-id="@Model" class="btn btn-info btn-sm">
        <i class="bi bi-eye"></i> Details
    </a>
</td>
<td>
    <a asp-action="Update" asp-route-id="@Model" class="btn btn-warning btn-sm mx-1">
        <i class="bi bi-pencil"></i> Update
    </a>
</td>
<td>
    <a asp-action="Delete" asp-route-id="@Model" class="btn btn-danger btn-sm">
        <i class="bi bi-trash"></i> Delete
    </a>
</td>
